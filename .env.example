# Configuration - Copy this to .env and update with your actual values
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/v1/chat/completions
N8N_AUTH_TOKEN=your-secure-n8n-token-here
REQUEST_TIMEOUT=120.0

# API Key for proxy authentication - Generate a strong random key
PROXY_API_KEY=sk-your-secure-proxy-api-key-here

# CORS Origins - Comma-separated list of allowed domains (use * for development only)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Rate Limiting - Requests per minute per IP address
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_WINDOW=60

# Debug Mode - Set to true for development only (exposes detailed errors)
DEBUG_MODE=false

# Security Headers - Set to false to disable specific headers if needed
SECURITY_HEADERS_ENABLED=true
